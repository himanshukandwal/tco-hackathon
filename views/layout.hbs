		</script>
		<script src="https://cdn.auth0.com/js/lock-7.1.min.js"></script>
		<script src="/bower_components/socket.io-client/socket.io.js"></script>
		<script type="text/javascript">
		    var lock = new Auth0Lock('{{auth0.clientId}}', '{{auth0.domain}}');
		    function login() {
		        lock.show({
		            dict: {
		              signin: {
		                title: "Login with your<br/>Topcoder account"
		              }
		            },
								usernameStyle: 'username',
								defaultUserPasswordConnection: 'LDAP',
		            signupLink: 'https://www.topcoder.com/register/?next={{auth0.tcCallbackUrl}}',
		            disableResetAction: true,
		            callbackURL: '{{auth0.callbackUrl}}'
		            , responseType: 'code'
		            , authParams: {
		                scope: '{{auth0.scope}}'
		            }
		        });
		    }
		</script>

	</head>

	<body>

		<!-- ============================================================= HEADER ============================================================= -->

		<header>
			<div class="navbar">

				<div class="navbar-header">
					<div class="container">

						<!-- ============================================================= LOGO MOBILE ============================================================= -->

						<a class="navbar-brand" href="/"><img src="/images/tco15hacks-logo.png" class="logo" alt="topcoder"></a>

						<!-- ============================================================= LOGO MOBILE : END ============================================================= -->

						<a class="btn responsive-menu pull-right" data-toggle="collapse" data-target=".navbar-collapse"><i class='icon-menu-1'></i></a>

					</div><!-- /.container -->
				</div><!-- /.navbar-header -->

				<div class="yamm">
					<div class="navbar-collapse collapse">
						<div class="container">

							<!-- ============================================================= LOGO ============================================================= -->

							<a class="navbar-brand" href="/"><img src="/images/tco15hacks-logo.png" class="logo" alt="topcoder"></a>

							<!-- ============================================================= LOGO : END ============================================================= -->


							<!-- ============================================================= MAIN NAVIGATION ============================================================= -->

							<ul class="nav navbar-nav">

								<li class="dropdown">
									<a href="#" class="dropdown-toggle js-activated">Hackathons</a>
									<ul class="dropdown-menu">
										<li><a href="/tco15-russia">TCO15 Russia</a></li>
										<li><a href="/tco15-usa">TCO15 USA</a></li>
										<li><a href="/tco15-japan">TCO15 Japan</a></li>
										<li><a href="/tco15-india">TCO15 India</a></li>
										<li><a href="/tco15-finals">TCO15 Finals</a></li>
									</ul><!-- /.dropdown-menu -->
								</li><!-- /.dropdown -->

								<li class="dropdown">
									<a href="#" class="dropdown-toggle js-activated">TopCoder Open</a>
									<ul class="dropdown-menu">
										<li><a href="http://tco15.topcoder.com/" target="_blank">2015 Topcoder Open</a></li>
										<li><a href="http://community.topcoder.com/tco14/" target="_blank">2014 Topcoder Open</a></li>
									</ul><!-- /.dropdown-menu -->
								</li><!-- /.dropdown -->

							</ul><!-- /.nav -->

							{{#if authenticated}}
								<a href="/logout" class="btn btn btn-orange">Logout</a>
							{{else}}
								<a href="#" onclick="login()" class="btn btn btn-orange">Login</a>
							{{/if}}

							<!-- ============================================================= MAIN NAVIGATION : END ============================================================= -->

						</div><!-- /.container -->
					</div><!-- /.navbar-collapse -->
				</div><!-- /.yamm -->
			</div><!-- /.navbar -->
		</header>

		<!-- ============================================================= HEADER : END ============================================================= -->


		<!-- ============================================================= MAIN ============================================================= -->

		<main>
			{{{body}}}
		</main>

		<!-- ============================================================= MAIN : END ============================================================= -->


		<!-- ============================================================= FOOTER ============================================================= -->

		<footer class="dark-bg">

			<div class="footer-bottom">
				<div class="container inner">
					<p class="pull-left">Â© 2015 Appirio. All rights reserved.</p>
					{{#if authenticated}}
						<p class="pull-right">Logged in as {{user.member.handle}}</p>
					{{/if}}
				</div><!-- .container -->
			</div><!-- .footer-bottom -->
		</footer>

